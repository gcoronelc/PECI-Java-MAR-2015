<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
	<servlet>
		<servlet-name>EmpleadoController</servlet-name>
		<servlet-class>controller.EmpleadoController</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>FinalizarSession</servlet-name>
		<servlet-class>controller.FinalizarSession</servlet-class>
	</servlet>
    <servlet>
        <servlet-name>CuentaController</servlet-name>
        <servlet-class>controller.CuentaController</servlet-class>
    </servlet>
    <servlet-mapping>
		<servlet-name>EmpleadoController</servlet-name>
		<url-pattern>/EmpleadoController</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>FinalizarSession</servlet-name>
		<url-pattern>/FinalizarSession</url-pattern>
	</servlet-mapping>
    <servlet-mapping>
        <servlet-name>CuentaController</servlet-name>
        <url-pattern>/CuentaController</url-pattern>
    </servlet-mapping>
    <session-config>
		<session-timeout>
            30
		</session-timeout>
	</session-config>
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
</web-app>
